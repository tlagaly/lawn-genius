{
  "name": "lawn-genius",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prisma:generate": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "generate-vapid-keys": "node scripts/generate-vapid-keys.js",
    "db:seed": "prisma db seed",
    "setup-test-db": "ts-node scripts/setup-test-db.ts",
    "test": "npm run setup-test-db && jest",
    "test:coverage": "npm run setup-test-db && jest --coverage --coverageReporters='json-summary' --coverageReporters='text' --coverageReporters='html'",
    "prepare": "npm install prisma --no-save && prisma generate && husky install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:auth": "cypress run --spec 'cypress/e2e/auth/**/*.cy.ts'",
    "cypress:run:lawn": "cypress run --spec 'cypress/e2e/lawn-profiles/**/*.cy.ts'",
    "cypress:run:species": "cypress run --spec 'cypress/e2e/grass-species/**/*.cy.ts'",
    "cypress:run:schedule": "cypress run --spec 'cypress/e2e/schedule/**/*.cy.ts'",
    "test:e2e": "start-server-and-test dev 3000 cypress:run",
    "test:e2e:ci": "start-server-and-test start 3000 cypress:run",
    "verify": "ts-node scripts/verify-deployment.ts",
    "verify:staging": "cross-env NODE_ENV=staging ts-node scripts/verify-deployment.ts",
    "verify:production": "cross-env NODE_ENV=production ts-node scripts/verify-deployment.ts",
    "build:staging": "cross-env NODE_ENV=staging next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "deploy:staging": "npm run verify:staging && npm run build:staging && npm run migrate:staging",
    "deploy:production": "npm run verify:production && npm run build:production && npm run migrate:production",
    "migrate:staging": "cross-env NODE_ENV=staging prisma migrate deploy",
    "migrate:production": "cross-env NODE_ENV=production prisma migrate deploy",
    "rollback:staging": "cross-env NODE_ENV=staging prisma migrate reset --force",
    "rollback:production": "cross-env NODE_ENV=production prisma migrate reset --skip-seed --force"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "^6.4.1",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@stripe/stripe-js": "^5.8.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.5",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.4.10",
    "micro": "^10.0.1",
    "next": "15.2.1",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.9.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "resend": "^4.1.2",
    "sonner": "^2.0.1",
    "stripe": "^17.7.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "web-push": "^3.6.7",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@cypress/code-coverage": "^3.13.12",
    "@eslint/eslintrc": "^3",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/dotenv": "^8.2.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "cypress": "^14.1.0",
    "cypress-localstorage-commands": "^2.2.7",
    "cypress-wait-until": "^3.0.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.35",
    "prisma": "^6.4.1",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
